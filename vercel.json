{
  "version": 2,
  "builds": [
    {
      "src": "api/publish.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/menus.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/schedule.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/cron.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/publish",
      "dest": "/api/publish.py"
    },
    {
      "src": "/api/menus",
      "dest": "/api/menus.py"
    },
    {
      "src": "/api/template",
      "dest": "/api/publish.py"
    },
    {
      "src": "/api/schedule",
      "dest": "/api/schedule.py"
    },
    {
      "src": "/api/cron",
      "dest": "/api/cron.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}